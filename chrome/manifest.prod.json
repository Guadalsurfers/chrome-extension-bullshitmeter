{
  "version": "0.0.0",
  "name": "react-chrome-extension-example",
  "manifest_version": 2,
  "description": "Example for react-chrome-extension-boilerplate",
  "browser_action": {
    "default_title": "React Chrome Extension Example",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "web_accessible_resources": [
    "inject.html"
  ],
  "background": {
    "page": "background.html"
  },
  "oauth2": {
   "client_id": "276988820197-ofigr0n79mvg022if4nfvph1jlu6gnq1.apps.googleusercontent.com",
   "scopes": ["https://www.googleapis.com/auth/plus.login"]
  }
  "permissions": [ "contextMenus", "identity", "tabs", "storage", "https://*/*", "http://*/*/"],
  "content_security_policy": "default-src 'self'; script-src 'self'; style-src * 'unsafe-inline'; connect-src https://bullshitmeter.herokuapp.com/; img-src 'self' data:;"
}
